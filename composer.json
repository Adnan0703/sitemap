{
    "name": "adnan0703/sitemap",
    "type": "cakephp-plugin",
    "description": "Sitemap plugin for CakePHP",
    "keywords": ["cakephp", "sitemap"],
    "homepage": "https://github.com/Adnan0703/sitemap",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Adnan Aslam",
            "email": "adnan.aslam0703@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev",
        "league/flysystem": "0.4.*",
        "tackk/cartographer": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Adnan0703\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adnan0703\\Sitemap\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
